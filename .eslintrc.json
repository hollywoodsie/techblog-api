{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:import/recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    
    "rules": {
        "no-underscore-dangle": "off",
        "import/extensions": [
            "off"],
        "import/prefer-default-export": "off",
        "no-console": "off"
        
        

    }
}
